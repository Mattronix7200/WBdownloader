      if (typeof console === "undefined") {
         var console = { log: function (logMsg) { } };
      }
      console.log(myData);

      var htmlText = '';
      htmlText += '<h1>Menadżer pobierania- WindowsBASE.pl</h1>';
      htmlText += '<h3>Kliknij w jeden z poniższych linków, by rozpocząć pobieranie</h3>';
      htmlText += '<div class="section">';
      for (var key in myData) {
         htmlText += '<div class="section2" key={index} onClick={handleRowClick} data-id={row.nazwa_obrazu}>';
         htmlText += '<p class="s2">Kategoria: ' + myData[key].kategoria + '</p>';
         htmlText += '<p class="s3"><b>Nazwa pliku: </b><i>' + myData[key].filename + '</i></p>';
         htmlText += myData[key].nazwa_obrazu;
         htmlText += '</div><hr class="collapse">';
   }
   htmlText += '</div">';
   
      $('body').append(htmlText); 

      var clipboard = new ClipboardJS('.btn'); 
      var clipboard = new ClipboardJS('.btn2'); 

      $(document).ready(function () {
         $(".btn").click(function () {
        $("#dialog").show();
        $("#dialog_bkg").show();
         });

         $("#aria").click(function () {
            $("#dialog_bkg").hide();
            $("#dialog").hide();
            var WshShell = new ActiveXObject("Wscript.Shell");
            WshShell.Run("%userprofile%\\AppData\\Local\\WBdownloader\\bin\\download_aria.exe");
         });

         $("#fdm").click(function () {
            $("#dialog_bkg").hide();
            $("#dialog").hide();
            var WshShell = new ActiveXObject("Wscript.Shell");
            WshShell.Run("%userprofile%\\AppData\\Local\\WBdownloader\\bin\\download_fdm.exe");
         });
         
       $("#jd").click(function () {
         $("#dialog_bkg").hide();
         $("#dialog").hide();
         var WshShell = new ActiveXObject("Wscript.Shell");
         WshShell.Run("%userprofile%\\AppData\\Local\\WBdownloader\\bin\\download_jd.exe");
         });
      });

      function grabMegaDL() {
         var WshShell = new ActiveXObject("Wscript.Shell");
         WshShell.Run("%userprofile%\\AppData\\Local\\WBdownloader\\bin\\download_jd.exe");
      }

      clipboard.on('success', function (e) {
         console.info('Action:', e.action);
         console.info('Text:', e.text);
         console.info('Trigger:', e.trigger);
         e.clearSelection();
      });

      clipboard.on('error', function (e) {
         console.error('Action:', e.action);
         console.error('Trigger:', e.trigger);
      });

      window.onerror = function () {
         return true
      } 

   </script>


